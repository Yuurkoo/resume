<br>
<br>
    
    {{#with recipients}}
    <div>
      <h1>{{title}}</h1>
    </div>
    <br>
    
    <div class="row">
      {{#each users}}
  <div class="col-6 ">
    <div class="card">
      <div class="card-body">
         <h5>{{name}}</h5>
        <p class="card-text">{{[phone]}}</p>
        {{#with button}}
        <a href="{{link}}" class="btn btn-primary">{{text}}</a>
        {{/with}}
      </div>
    </div>
  </div>
  {{/each}}
</div>

 {{/with}}

      {{/with}}
    </section>


   
</main>