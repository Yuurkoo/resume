<main>
      {{#with navigation}}
    <section class="card">
        <div class="nav">
            {{#each links}}
            <div class="nav-item">
                <a href="{{href}}" class="nav-link">{{text}}</a>
            </div>
            {{/each}}
            <div class="nav-item">
                <a href="#" class="nav-link disabled">Disabled</a>
            </div>
        </div>
    </section>
    {{/with}}

    <br>

    <div class="mt-5">
      <h1>Мої замовлення</h1>
    </div>

    <section>
      {{#with purchase}}
      <div> 
      <ul class="nav nav-tabs">
         <li class="nav-item ">
            <a class="nav-link active" href="#">Всі замовлення</a>
         </li>
         <li class="nav-item ">
            <a class="nav-link " href="#">Гарантія та повернення</a>
         </li>
      </ul>
      </div>
      <div>
        {{#with actionBlock}}
        <nav class="navbar row">
            <div class="col-12">
              <form class="d-flex mt-3">
                {{#with input}}
                 <input class="form-control me-2"  placeholder="{{placeholder}}">
                 {{/with}}
                 <div class="col-10">
                 {{#each buttons}}
                    {{#if isSuccess}}<a href="{{link}}" class="btn btn-success me-2">{{text}}</a>{{/if}}
                    {{#if isOutline}}<a href="{{link}}" class="btn btn-outline-success me-2">{{text}}</a>{{/if}}
                    {{#if isDanger}}<a href="{{link}}" class="btn btn-outline-danger">{{text}}</a>{{/if}}
                 {{/each}}
                 </div>
             </form>
             </div>
        </nav>
        {{/with}}
      </div>

       <div>
      {{#each itemList}}
       <div class="card mb-3" >
  <div class="row g-0">
    <div class="col-md-3">
      <div class="card-body">    
         <p>{{number}}</p>
         <p><strong>{{status}}</strong></p>
      </div>
    </div>
    <div class="col-md-2 ps-5">
        {{#with amount}}
      <div class="card-body ps-5 mt-2">
        <p class="card-title">{{title}}</p>
        <p class="card-text"><strong>{{value}}</strong></p>
      </div>
      {{/with}}
    </div>
    {{#each images}}
    <div class="col-1 g-1 pt-1">
      <img src="{{this}}" class="img-fluid rounded-start" alt="alt">
    </div>
    {{/each}}
    <div class="text-end col-3">
        {{#with button}}
        <div class="pe-5 pt-5">
           {{#if isPrimary}}<a href="{{link}}" class="btn btn-primary">{{text}}</a>{{/if}}
         </div>
        {{/with}}
    </div>
  </div>
 </div>
</div>
      {{/each}}
    

      {{/with}}
    </section>

    <br>
    <div>
      <hr>
    </div>
    <br>

    <section>
      {{#with userInfo}}
      <div>
      <h1>{{title}}</h1>
      </div>

      <br>
      <br>

      <div>
         {{#with profileData}}
          {{#with fullName}}
          <div class="row">
        <div class="card col-12">
            <div class=" row g-4">
                <div class="col-4">
                    {{#with surname}}
                   <p>{{title}}</p>
                      <p>{{value}}</p>
                   {{/with}}
                </div>
                <div class="col-4">
                    {{#with name}}
                   <p>{{title}}</p>
                      <p>{{value}}</p>
                   {{/with}}
                </div>
                <div class="col-4">
                    {{#with middleName}}
                  <p>{{title}}</p>
                      <p>{{value}}</p>
                   {{/with}}
                </div>
            </div>
        </div>
    </div>
    {{/with}}

    </div>
          <div class="mt-3">
         {{#with otherInfo}}
          <div class="row">
        <div class="card col-12">
            <div class=" row g-4">
                <div class="col-4">
                    {{#with birthday}}
                   <p>{{title}}</p>
                      <p>{{value}}</p>
                   {{/with}}
                </div>
                <div class="col-4">
                    {{#with sex}}
                   <p>{{title}}</p>
                      <p>{{value}}</p>
                   {{/with}}
                </div>
                <div class="col-4">
                    {{#with language}}
                  <p>{{title}}</p>
                      <p>{{value}}</p>
                   {{/with}}
                </div>
            </div>
        </div>
    </div>
    {{/with}}
    </div>
    {{/with}}



